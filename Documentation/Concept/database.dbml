Project svr {
  database_type: 'MySQL'
}

Enum "teams_type_enum" {
  "betrieb"
  "feuerwehr"
  "verein"
}

Enum "competition_type_enum" {
  "betrieb"
  "feuerwehr"
  "pokal"
  "runden"
}

Table "persons" {
  "uid" int(11) [pk, not null]
  "first_name" varchar(255) [default: NULL]
  "last_name" varchar(255) [default: NULL]
  "profi" tinyint(1) [not null]
}

Table "person_team_mm" {
  "person" int(11) [not null]
  "team" int(11) [not null]
}

Table "shots" {
  "uid" int(11) [pk, not null]
  "value" float [not null]
  "x" float [not null]
  "y" float [not null]
  "time" datetime [not null, default: `current_timestamp()`]
}

Table "teams" {
  "uid" int(11) [pk, not null]
  "name" varchar(255) [not null]
  "type" teams_type_enum [not null]
  "single_shooter" int [default: null]
  "competition" int [not null]
}

Table "competitions" {
  "uid" int(11) [pk, not null]
  "name" varchar [not null]
  "type" competition_type_enum
  "start" datetime [not null]
  "end" datetime [not null]
}

Table "competitions_disciplines_mm" {
  "competition" int(11) [not null]
  "discipline" int(11) [not null]
}

Table "competitions_rounds_mm" {
  "competition" int(11) [not null]
  "round" int(11) [not null]
}

Table "disciplines" {
  "uid" int(11) [pk, not null]
  "name" varchar [not null]
}

Table "series" {
  "uid" int(11) [pk, not null]
  "person" int [not null]
  "dicipline" int [not null]
  "round" int [not null]
  "team" int

}

Table "series_shots_mm" {
  "series" int [not null]
  "shot" int [not null]
}

Table "rounds" {
  "uid" int [not null]
  "name" varchar
  "start" datetime [not null]
  "end" datetime [not null]
}




Ref: "person_team_mm"."person" > "persons"."uid"

Ref: "person_team_mm"."team" > "teams"."uid"

Ref: "teams"."competition" > "competitions"."uid"

Ref: "competitions_disciplines_mm"."discipline" > "disciplines"."uid"

Ref: "competitions_disciplines_mm"."competition" > "competitions"."uid"

Ref: "series"."person" > "persons"."uid"

Ref: "series"."dicipline" > "disciplines"."uid"

Ref: "teams"."single_shooter" > "persons"."uid"

Ref: "series_shots_mm"."shot" > "shots"."uid"

Ref: "series_shots_mm"."series" > "series"."uid"

Ref: "series"."round" > "rounds"."uid"

Ref: "competitions_rounds_mm"."competition" > "competitions"."uid"

Ref: "competitions_rounds_mm"."round" > "rounds"."uid"

Ref: "series"."team" > "teams"."uid"
